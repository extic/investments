    "pinia": "^2.0.27",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.1.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "electron": "^22.0.0",
    "electron-builder": "^23.3.3",
    "sass": "^1.56.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-plugin-electron": "^0.10.4",
    "vite-plugin-electron-renderer": "^0.12.1",
    "vue": "^3.2.40",
    "vue-tsc": "^1.0.9"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
